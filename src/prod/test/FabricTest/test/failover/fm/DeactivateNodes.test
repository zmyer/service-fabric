#############
# Scenario 8: FM will send NodeDeactivationRequest to the node which has no up replicas
FM will not mark node as DeactivationComplete until receiving reply from RA
#############

createservice fabric:/stateless8 CalculatorServiceType n 1 3
createservice fabric:/volatile8 TestStoreServiceType y 1 3 minreplicasetsize=2
createservice fabric:/persisted8 TestPersistedStoreServiceType y 1 3 persist minreplicasetsize=2
verify

+80
verify

# Verify there's no replicas on node 80
!waitforstate FM.UpReplicaCount.80 0

addbehavior b1 * 80 NodeDeactivateRequest

# Deactivate node 80 with intent Restart
DeactivateNode 80 Restart

# RA will not send reply to FM so DeactivationStatus will not be Complete
!waitforstate FM.Node.DeactivationIntent.80 Restart
!waitforstate FM.Node.DeactivationStatus.80 DeactivationSafetyCheckComplete

# Wait for some time and DeactivationStatus is still not Complete since NodeDeactivateRequest is blocked
!pause 10
!waitforstate FM.Node.DeactivationStatus.80 DeactivationSafetyCheckComplete

# RA sends the reply to FM and DeactivationStatus becomes Complete
removebehavior b1
!waitforstate FM.Node.DeactivationStatus.80 DeactivationComplete

deleteservice fabric:/stateless8
deleteservice fabric:/persisted8
deleteservice  fabric:/volatile8
verify

-80
verify

#############
# Scenario 9: RA will not send NodeDeactivateReply to FM until all replicas are closed
#############

createservice fabric:/stateless9 CalculatorServiceType n 1 3
createservice fabric:/volatile9 TestStoreServiceType y 1 3 minreplicasetsize=2
createservice fabric:/persisted9 TestPersistedStoreServiceType y 1 3 persist minreplicasetsize=2
verify

!waitforstate RA.Replica.IsUp.fabric:/stateless9.70.70 true
!waitforstate RA.Replica.IsUp.fabric:/volatile9.70.70 true
!waitforstate RA.Replica.IsUp.fabric:/persisted9.70.70 true

addbehavior b1 * * ReplicaClose

# Deactivate node 70 with intent Restart
DeactivateNode 70 Restart

!waitforstate FM.Node.DeactivationIntent.70 Restart
!waitforstate FM.Node.DeactivationStatus.70 DeactivationSafetyCheckComplete

# Wait for some time and DeactivationStatus is still not Complete since NodeDeactivateRequest is blocked
!pause 10
!waitforstate FM.Node.DeactivationStatus.70 DeactivationSafetyCheckComplete

addbehavior b2 * * NodeDeactivateReply
removebehavior b1

# Replicas are Closed on node 70
!waitforstate RA.Replica.IsUp.fabric:/stateless9.70.70 false
!waitforstate RA.Replica.IsUp.fabric:/volatile9.70.70 false
!waitforstate RA.Replica.IsUp.fabric:/persisted9.70.70 false

# DeactivationStatus is still not Complete since NodeDeactivateReply is blocked
!waitforstate FM.Node.DeactivationStatus.70 DeactivationSafetyCheckComplete

# RA sends the reply to FM and DeactivationStatus becomes Complete
removebehavior b2
!waitforstate FM.Node.DeactivationStatus.70 DeactivationComplete

deleteservice fabric:/stateless9
deleteservice fabric:/persisted9
deleteservice  fabric:/volatile9
verify

